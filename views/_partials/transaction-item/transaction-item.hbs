<a type="button" class="transaction-item {{#if itemData.hasUpgrade}}transaction-item--upgrade{{/if}}" {{#if itemData.hasUpgrade}}data-transaction-item-upgrade="{{itemData.name}}"{{/if}}>
  <div class="transaction-item__content">
    <p class="transaction-item__name">
      {{ itemData.name }}
    </p>
    <p class="transaction-item__amount">
      &#163;{{ itemData.amount }}
    </p>
    <p class="transaction-item__type">
      {{ itemData.type }}
    </p>
  </div>
</a>
